{
  "auto_update": false,
  "edit_predictions": {
    "mode": "eager"
  },
  "agent": {
    "default_profile": "write",
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    },
    "inline_assistant_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    },
    "model_parameters": []
  },
  "languages": {
    "V": {
      "auto_indent": true
    },
    "Zig": {
      "show_edit_predictions": true
    },
    "Odin": {
      "show_edit_predictions": true
    }
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },

  "autosave": { "after_delay": { "milliseconds": 500 } },
  "terminal": { "toolbar": { "breadcrumbs": false } },
  "restore_on_startup": "none",
  "extend_comment_on_newline": false,
  "auto_signature_help": true,
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  }
}
