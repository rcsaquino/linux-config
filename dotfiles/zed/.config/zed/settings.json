{
  "edit_predictions": {
    "mode": "subtle"
  },
  "agent": {
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-flash-lite-preview"
    },
    "inline_assistant_model": {
      "provider": "google",
      "model": "gemini-2.5-flash-lite-preview"
    },
    "model_parameters": []
  },
  "languages": {
    "Odin": {
      "show_edit_predictions": true
    }
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },

  "autosave": { "after_delay": { "milliseconds": 500 } },
  "terminal": { "toolbar": { "breadcrumbs": false } },
  "restore_on_startup": "none",
  "extend_comment_on_newline": false
}
